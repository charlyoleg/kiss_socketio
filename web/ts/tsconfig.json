{
  "compilerOptions": {
    "module": "es6",
    "target": "es6",
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noImplicitReturns": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "typeRoots": ["node_modules/@types"],
    "removeComments": false,
    "outDir": "../dist/js"
  },
  "include": [
    "**/*"
  ]
}
