{
  "name": "kiss_socketio",
  "version": "1.0.0",
  "description": "playing with socket.io",
  "main": "srv/dist/index.js",
  "scripts": {
    "clean_web": "rm -fr web/dist",
    "build_web_html": "pug --pretty --out web/dist/ web/pug/index.pug",
    "build_web_js": "tsc --project web/ts",
    "build_web_css": "sass web/sass/kiss_socketio_style.scss web/dist/css/kiss_socketio_style.css",
    "build_web": "run-p -l build_web_html build_web_js build_web_css",
    "serve_web": "live-server web/dist",
    "start": "run-s clean_web build_web serve_web",
    "clean_srv_http_ws": "rm -fr srv_http_ws/dist",
    "build_srv_http_ws_js": "tsc -p srv_http_ws",
    "build_srv_http_ws_cp_html": "ncp web/dist/index.html srv_http_ws/dist/index.html --stopOnErr",
    "build_srv_http_ws_cp_css": "ncp web/dist/css/kiss_socketio_style.css srv_http_ws/dist/kiss_socketio_style.css --stopOnErr",
    "build_srv_http_ws_cp_js": "ncp web/dist/js/kiss_socketio.js srv_http_ws/dist/kiss_socketio.js --stopOnErr",
    "build_srv_http_ws_cp": "run-p -l build_srv_http_ws_cp_html build_srv_http_ws_cp_css build_srv_http_ws_cp_js",
    "build_srv_http_ws": "run-s build_srv_http_ws_js build_srv_http_ws_cp",
    "serve_http_ws": "node srv_http_ws/dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charlyoleg/kiss_socketio.git"
  },
  "author": "charlyoleg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/charlyoleg/kiss_socketio/issues"
  },
  "homepage": "https://github.com/charlyoleg/kiss_socketio#readme",
  "dependencies": {
    "@types/node": "^12.7.2",
    "express": "^4.17.1",
    "live-server": "^1.2.1",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.22.10",
    "socket.io": "^2.2.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {}
}
