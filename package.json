{
  "name": "kiss_socketio",
  "version": "1.0.0",
  "description": "playing with socket.io",
  "main": "srv/dist/index.js",
  "scripts": {
    "clean_web": "rm -fr web/dist",
    "build_web_html": "pug --pretty --out web/dist/ web/pug/index.pug",
    "build_web_js": "tsc --project web/ts",
    "build_web_cp_js": "mkdir -p web/dist/socket.io && cpx web/socketio_js/socket.io.js web/dist/socket.io/",
    "build_web_css": "sass web/sass/kiss_socketio_style.scss web/dist/css/kiss_socketio_style.css",
    "build_web": "run-p -l build_web_html build_web_js build_web_cp_js build_web_css",
    "serve_web": "live-server web/dist",
    "start": "run-s clean_web build_web serve_web",
    "clean_srv": "rm -fr srv/dist",
    "build_srv_js": "tsc -p srv",
    "build_srv_cp_html": "cpx web/dist/index.html srv/dist/",
    "build_srv_cp_css": "cpx web/dist/css/kiss_socketio_style.css srv/dist/",
    "build_srv_cp_js": "cpx web/dist/js/kiss_socketio.js srv/dist/",
    "build_srv_cp": "run-p -l build_srv_cp_html build_srv_cp_css build_srv_cp_js",
    "build_srv": "run-s build_srv_js build_srv_cp",
    "serve": "node srv/dist/index.js",
    "resrv_html_ws": "run-s clean_web build_web clean_srv build_srv serve",
    "resrv_ws": "run-s clean_srv build_srv_js serve",
    "create_ssl_keys": "srv/create_ssl_keys.sh",
    "clean_client_js": "rm -fr client_js/dist",
    "build_client_js": "tsc -p client_js",
    "run_client_js": "node client_js/dist/client.js",
    "rerun_client_js": "run-s clean_client_js build_client_js run_client_js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charlyoleg/kiss_socketio.git"
  },
  "author": "charlyoleg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/charlyoleg/kiss_socketio/issues"
  },
  "homepage": "https://github.com/charlyoleg/kiss_socketio#readme",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.2",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cpx": "^1.5.0",
    "express": "^4.17.1",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.22.10",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {}
}
